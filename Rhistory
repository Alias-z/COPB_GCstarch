#model <- lm(`TA90 (min)` ~ `Category` + `Photosynthetic pathway` + `Climate (3 groups)` + `Low light intensity` + `Hight light intensity` + `gsw_ini` + `gsw_ini / gsw_max` +`Tgs90 (min)`, data = species_data)
grass_data <- species_data %>% dplyr::filter(`Category` == 'Angiosperm (Monocot)')
non_grass <- species_data %>% dplyr::filter(`Category` != 'Angiosperm (Monocot)')
model <- lm(`TA90 (min)` ~ `Category` + `Photosynthetic pathway`+  `Climate (3 groups)` + `Low light intensity` + `Hight light intensity` + `gsw_ini (mmol m-2 s-1)` + `gsw_ini / gsw_max` +`Tgs90 (min)`, data = species_data)
#summary(model)
plot_t90_side_by_side_box(species_data)
#| label: setup
#| echo: false
#| message: false
options(warnParticalMatchDollar = TRUE)  # warning $ partial matching
options(warnParticalMatchArgs = TRUE)  # warning func args partial matching
